<form action="/register" method="post">
  <div>
    <label for="username">Username</label>
    <input
      id="username"
      name="username"
      type="text"
      autocomplete="off"
      required
    />
  </div>
  <div>
    <label for="current-password">Password</label>
    <input
      id="current-password"
      name="password"
      type="password"
      autocomplete="off"
      required
    />
  </div>
  <div>
    <button type="submit">Register</button>
  </div>
</form>
